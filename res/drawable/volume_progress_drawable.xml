<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <size android:height="6.0dip" android:width="6.0dip" />
            <gradient android:startColor="#ff242424" android:endColor="#ff4c4c4c" android:angle="270.0" />
            <stroke android:width="1.0px" android:color="#154c4c4c" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="3.0dip" />
                <size android:height="6.0dip" />
                <gradient android:startColor="#ff4f6b00" android:endColor="#ff7da905" android:angle="90.0" />
                <stroke android:width="1.0px" android:color="#157da905" />
            </shape>
        </clip>
    </item>
</layer-list>