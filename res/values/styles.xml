    <!-- <item name="android:textColor">@drawable/txt_cell_queue_subtitle</item> -->
    <!-- <item name="android:paddingLeft">9.0dip</item> -->
    <!-- </style> -->

    <style name="PlayerSeekbar">
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">6.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:progress">0</item>
        <item name="android:progressDrawable">@drawable/player_progress_drawable</item>
        <item name="android:thumb">@drawable/player_progress_thumb</item>
        <item name="android:thumbOffset">9.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="PlayerPlayBar">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/player_play_bar_height</item>
    </style>

    <style name="PlayerBarCompatItem.Player" parent="@style/PlayerBarCompatItem">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="PlayerOperationBarItem">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">42.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style name="TrackMenu.Player" parent="@style/TrackMenu">
        <item name="android:textColor">#fff5f5f5</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="PlayerSubTitleMultiLine" parent="@style/PlayerSubTitle">
        <item name="android:textSize">14.0dip</item>
        <item name="android:ellipsize">none</item>
        <item name="android:singleLine">false</item>
    </style>

</resources>